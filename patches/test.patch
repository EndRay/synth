load "init.patch"

-v-
env = new ADSR(0.2s, 2s, 0.7, 0.2s)
env.gate <= gate
env.trig <= trigger

lfo = new Sine(10s)
lfo.sync <= trigger

osc = new Pulse(pitch)
osc.pw <- lfo * 0.2
osc * env => output

filter = new Filter(0.3)
filter.f <- pitch
filter.f <- lfo * -0.3
env * 0.3 -> filter.f
filter ->- output
---

volume := 0.04